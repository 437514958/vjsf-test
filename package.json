    "jsonpointer": "^5.0.1",
    "jss": "^10.10.0",
    "jss-preset-default": "^10.10.0",
    "lodash.topath": "^4.5.2",
    "lodash.union": "^4.6.0",
    "vue": "^3.3.4",
    "vue-jss": "^0.0.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
