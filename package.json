    "@vue/tsconfig": "^0.4.0",
    "circular-dependency-plugin": "^5.2.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "jsdom": "^22.1.0",
    "monaco-editor": "^0.44.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "npm-run-all2": "^6.0.6",
